# velocity: Optional[float] = None,
# acceleration: Optional[float] = None,
# gripper_speed: Optional[float] = None,
# gripper_force: Optional[float] = None,

# tcp_pose:
# - 0.00256
# - -0.00115
# - 0.1506
# - -0.0203
# - -3.0925
# - 0.0024

record(ao, "$(P)UR:MoveParams:TCPOffset_X") {
    # field(VAL, 0.00256)
    field(PREC, 6)
}

record(ao, "$(P)UR:MoveParams:TCPOffset_Y") {
    # field(VAL, -0.00115)
    field(PREC, 6)
}

record(ao, "$(P)UR:MoveParams:TCPOffset_Z") {
    # field(VAL, 0.1506)
    field(PREC, 6)
}

record(ao, "$(P)UR:MoveParams:TCPOffset_Roll") {
    # field(VAL, -0.0203)
    field(PREC, 6)
}

record(ao, "$(P)UR:MoveParams:TCPOffset_Pitch") {
    # field(VAL, -3.0925)
    field(PREC, 6)
}

record(ao, "$(P)UR:MoveParams:TCPOffset_Yaw") {
    # field(VAL, 0.0024)
    field(PREC, 6)
}

record(ao, "$(P)UR:MoveParams:Velocity") {
    # field(VAL, 0.05)
    field(PREC, 6)
}

record(ao, "$(P)UR:MoveParams:Acceleration") {
    # field(VAL, 0.1)
    field(PREC, 6)
}

record(ao, "$(P)UR:MoveParams:GripperForce") {
    field(PREC, 6)
}

record(ao, "$(P)UR:MoveParams:GripperSpeed") {
    field(PREC, 6)
}

record(bo, "$(P)UR:MoveParams:Run") {
    field(FLNK, "$(P)UR:MoveParams:set_post_url.PROC")
}

record(luascript, "$(P)UR:MoveParams:set_post_url") {
    field(CODE, "@ur_module.lua move_params_set_post_url({P='$(P)',host='$(HOST)'})")
    field(SYNC, "Sync")
    field(OUT, "$(P)$(R1)URL.VAL$ PP")
    field(FLNK, "$(P)$(R1)Post.PROC")
}
